  display: -webkit-flex; /* Safari */
  flex-direction: row ;
  flex-wrap: wrap ;

}
.dian_ming{
  width: 75%;
  box-sizing: border-box;
  border-right: 1px solid 	#D9D9D9;
  height: 50px;
  line-height: 50px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dian_ming image{
