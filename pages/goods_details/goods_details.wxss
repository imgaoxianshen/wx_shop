}
.title{
  width: 100%;
  height: 45px;
  line-height: 45px;
   display: flex;
  display: -webkit-flex; /* Safari */
  flex-direction: row ;
  flex-wrap: wrap ;
  margin: 10px 0px;
}
.title_goods{
  box-sizing: border-box;
  width: 80%;
  border-right: 1px solid 	#D9D9D9;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 16px;
  padding-left: 20rpx;
}
.title_fen{
  width: 20%;
  text-align: center;
  font-size: 14px;
}
.title_fen image{
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: -10rpx;
}
