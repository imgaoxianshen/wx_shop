height: 25px;
line-height: 25px;
}
.content{
  width: 100%;
}
.content image{
  width: 100%;
}

.foot{
   position: fixed;
  bottom: 0px; 
  display: flex;
  display: -webkit-flex; /* Safari */
  flex-direction: row ;
  flex-wrap: wrap ;
  height: 50px;
  width: 100%;
  background: white;
  border-top: 1px solid #D9D9D9;
}
.f_one,.f_two,.f_three{
  width: 14%;
  border-right: 1px solid #D9D9D9;
  height: 50px;
  box-sizing: border-box;
  text-align: center
}
.f_four,.f_five{
  width: 29%;
  border-right: 1px solid #D9D9D9;
  height: 50px;
  box-sizing: border-box;
  text-align: center;
  line-height: 50px;
  font-size: 14px;
}
.foot image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 10rpx;
}
.foot_title{
  font-size: 22rpx;
  margin-top: -8rpx
}
.active{
  background: #7B418B;
  color: white
}
.maskLayer{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 1;
  top: 0px;
  background: #080808;
  opacity: 0.6

}
.choose{
  width: 100%;
  position: fixed;
  z-index: 2;
  bottom: 0px;
  background: white
}

.shuoming{
  width: 92%;
